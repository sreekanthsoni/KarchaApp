        <icon height="87" width="87" platform="ios" src="www/res/ios/Icon-App-29x29@3x.png"/>
        <icon height="40" width="40" platform="ios" src="www/res/ios/Icon-App-40x40@1x.png"/>
        <icon height="80" width="80" platform="ios" src="www/res/ios/Icon-App-40x40@2x.png"/>
        <icon height="120" width="120" platform="ios" src="www/res/ios/Icon-App-40x40@3x.png"/>
        <icon height="57" width="57" platform="ios" src="www/res/ios/Icon-App-57x57@1x.png"/>
        <icon height="114" width="114" platform="ios" src="www/res/ios/Icon-App-57x57@2x.png"/>
