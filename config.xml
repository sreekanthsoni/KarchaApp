        <icon height="152" width="152" platform="ios" src="www/res/ios/Icon-App-76x76@2x.png"/>
        <icon height="50" width="50" platform="ios" src="www/res/ios/Icon-Small-50x50@1x.png"/>
        <icon height="100" width="100" platform="ios" src="www/res/ios/Icon-Small-50x50@2x.png"/>
        <splash height="512" width="512" platform="ios" src="www/res/ios/iTunesArtWork@1x.png"/>
