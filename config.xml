<?xml version='1.0' encoding='utf-8'?>
<widget id="in.co.bhst.karcha" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>Karcha</name>
    <description>
        APPLICATION FOR THE MAINTAINING THE EXPENSE.
    </description>
    <author email="srikanthsoni@bhst.co.in" href="http://bhst.co.in">
        BHST TEAM
    </author>
    <gap:plugin name="com.phonegap.plugin.statusbar" />
    <content src="index.html" />
    <preference name="fullscreen" value="true" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="14" />
    <preference name="StatusBarOverlaysWebView" value="true" />
    <preference name="StatusBarBackgroundColor" value="#000000" />
    <preference name="StatusBarStyle" value="blacktranslucent" />
    <access origin="*" />
    <platform name="android">
        <icon density="ldpi" src="www/res/android/ldpi/ic_launcher.png" />
        <icon density="mdpi" src="www/res/android/mdpi/ic_launcher.png" />
        <icon density="hdpi" src="www/res/android/hdpi/ic_launcher.png" />
        <icon density="xhdpi" src="www/res/android/xhdpi/ic_launcher.png" />
        <icon density="xxhdpi" src="www/res/android/xxhdpi/ic_launcher.png" />
        <icon density="xxxhdpi" src="www/res/android/xxxhdpi/ic_launcher.png" />
        <splash density="land-ldpi" src="www/res/android/ldpi/ic_launcher.png" />
        <splash density="land-mdpi" src="www/res/android/mdpi/ic_launcher.png" />
        <splash density="land-hdpi" src="www/res/android/hdpi/ic_launcher.png" />
        <splash density="land-xhdpi" src="www/res/android/xhdpi/ic_launcher.png" />
        <splash density="land-xxhdpi" src="www/res/android/xxhdpi/ic_launcher.png" />
