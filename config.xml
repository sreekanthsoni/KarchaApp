        <splash density="port-mdpi" src="www/res/android/mdpi/ic_launcher.png" />
        <splash density="port-hdpi" src="www/res/android/hdpi/ic_launcher.png" />
        <splash density="port-xhdpi" src="www/res/android/xhdpi/ic_launcher.png" />
        <splash density="port-xxhdpi" src="www/res/android/xxhdpi/ic_launcher.png" />
        <splash density="port-xxxhdpi" src="www/res/android/xxxhdpi/ic_launcher.png" />
    </platform>
    <platform name="ios">
        <icon height="20" width="20" platform="ios" src="www/res/ios/Icon-App-20x20@1x.png"/>
        <icon height="40" width="40" platform="ios" src="www/res/ios/Icon-App-20x20@2x.png"/>
        <icon height="60" width="60" platform="ios" src="www/res/ios/Icon-App-20x20@3x.png"/>
        <icon height="29" width="29" platform="ios" src="www/res/ios/Icon-App-29x29@1x.png"/>
